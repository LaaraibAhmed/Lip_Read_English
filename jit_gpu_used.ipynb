    "from numba import jit, cuda\n",
    "import numpy as np\n",
    "@jit(target_backend='cuda') \n",
    "def func2(a):\n",
    "    for i in range(100000000):\n",
    "        a[i]+= 1\n",
    "if __name__==\"__main__\":\n",
    "    n = 100000000                      \n",
    "    a = np.ones(n, dtype = np.float64)  \n",
    "    func2(a)    "
   ]
  }
